
<% unless node[:rabbitmq].nil? -%>
command[check_rabbitmq]=/usr/lib/nagios/plugins/check_rabbitmq_aliveness -H <%= node['ipaddress'] %> --port 15672 -u <%= node[:rabbitmq][:user] %> -p <%= node[:rabbitmq][:password] %> --vhost <%= node[:rabbitmq][:vhost] %>
command[check_rabbitmq_cluster]=/usr/lib/nagios/plugins/check_rabbit_cluster -h <%= node['ipaddress'] %> -t 15672 -u <%= node[:rabbitmq][:user] %> -p <%= node[:rabbitmq][:password] %> -n 3
<% end -%>
